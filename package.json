    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "expo": "53.0.22",
    "expo-application": "~6.1.5",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
