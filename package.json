{
  "name": "lockerroom-talk-app",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint --config eslint.config.js .",
    "build:web": "expo export --platform web",
    "build:web:prod": "NODE_ENV=production expo export --platform web",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "performance:test": "npm run build:web && npm run analyze-bundle",
    "clean": "expo r -c",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "deploy": "node scripts/deploy.js",
    "deploy:check": "node scripts/pre-deployment-checklist.js",
    "deploy:firebase": "firebase deploy",
    "deploy:web": "npm run build:web:prod && firebase deploy --only hosting",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
